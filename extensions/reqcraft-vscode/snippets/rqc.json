{
  "Import": {
    "prefix": "import",
    "body": ["import \"${1:./path}\""],
    "description": "Import file"
  },
  "Config": {
    "prefix": "config",
    "body": ["config {", "\tbaseUrl ${1:http://localhost:3000}", "}"],
    "description": "Configuration block"
  },
  "Variable": {
    "prefix": "variable",
    "body": ["variable ${1:name}${2: ${3|String,Number,Boolean|}}${4: default(\"${5:value}\")}"],
    "description": "Global variable definition"
  },
  "Variable Simple": {
    "prefix": "var",
    "body": ["variable ${1:name}"],
    "description": "Simple variable (String type)"
  },
  "Variable with Default": {
    "prefix": "vard",
    "body": ["variable ${1:name} default(\"${2:value}\")"],
    "description": "Variable with default value"
  },
  "Header": {
    "prefix": "header",
    "body": ["header ${1:Authorization}${2: @default(\"${3:Bearer token}\")}"],
    "description": "Global header definition"
  },
  "Header Simple": {
    "prefix": "hdr",
    "body": ["header ${1:Authorization}"],
    "description": "Simple header definition"
  },
  "API": {
    "prefix": "api",
    "body": [
      "api ${1:/path} {",
      "\t${2|get,post,put,delete,patch|} {",
      "\t\tname \"${3:API Name}\"",
      "\t\tresponse {",
      "\t\t\t$0",
      "\t\t}",
      "\t}",
      "}"
    ],
    "description": "API endpoint"
  },
  "Category": {
    "prefix": "category",
    "body": ["category ${1:name} {", "\tname \"${2:Display Name}\"", "\t$0", "}"],
    "description": "API category"
  },
  "GET": {
    "prefix": "get",
    "body": ["get {", "\tname \"${1:Get}\"", "\tresponse {", "\t\t$0", "\t}", "}"],
    "description": "GET method"
  },
  "POST": {
    "prefix": "post",
    "body": ["post {", "\tname \"${1:Create}\"", "\trequest {", "\t\t$2", "\t}", "\tresponse {", "\t\t$0", "\t}", "}"],
    "description": "POST method"
  },
  "Request": {
    "prefix": "request",
    "body": ["request {", "\t$0", "}"],
    "description": "Request schema"
  },
  "Response": {
    "prefix": "response",
    "body": ["response {", "\t$0", "}"],
    "description": "Response schema"
  },
  "Field String": {
    "prefix": "fstr",
    "body": ["${1:name} String"],
    "description": "String field"
  },
  "Field Number": {
    "prefix": "fnum",
    "body": ["${1:name} Number"],
    "description": "Number field"
  },
  "Field Nested": {
    "prefix": "fnested",
    "body": ["${1:name} {", "\t$0", "}"],
    "description": "Nested object"
  },
  "@mock": {
    "prefix": "@mock",
    "body": ["@mock(${1:\"value\"})"],
    "description": "Mock value"
  },
  "@example": {
    "prefix": "@example",
    "body": ["@example(${1:\"value\"})"],
    "description": "Example value"
  },
  "@params": {
    "prefix": "@params",
    "body": ["@params"],
    "description": "Query parameter"
  },
  "@default": {
    "prefix": "@default",
    "body": ["@default(\"${1:value}\")"],
    "description": "Default value for header"
  },
  "WebSocket": {
    "prefix": "ws",
    "body": [
      "ws ${1:wss://example.com/} {",
      "\tevent ${2:eventName} {",
      "\t\trequest {",
      "\t\t\t$3",
      "\t\t}",
      "\t\tresponse {",
      "\t\t\t$0",
      "\t\t}",
      "\t}",
      "}"
    ],
    "description": "WebSocket endpoint"
  },
  "Event": {
    "prefix": "event",
    "body": [
      "event ${1:eventName} {",
      "\trequest {",
      "\t\t$2",
      "\t}",
      "\tresponse {",
      "\t\t$0",
      "\t}",
      "}"
    ],
    "description": "WebSocket event"
  },
  "SocketIO": {
    "prefix": "socketio",
    "body": [
      "socketio ${1:http://localhost:3000/} {",
      "\tevent ${2:eventName} {",
      "\t\trequest {",
      "\t\t\t$3",
      "\t\t}",
      "\t\tresponse {",
      "\t\t\t$0",
      "\t\t}",
      "\t}",
      "}"
    ],
    "description": "SocketIO endpoint"
  }
}
